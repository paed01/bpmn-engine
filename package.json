{
  "name": "bpmn-engine",
  "description": "BPMN 2.0 execution engine. Open source javascript workflow engine.",
  "version": "8.5.5",
  "main": "dist/index.js",
  "source": "index.js",
  "module": "dist/index.esm.js",
  "umd:mainDist": "dist/bpmn-engine.umd.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/paed01/bpmn-engine"
  },
  "author": {
    "name": "PÃ¥l Edman, Mario Mol <mariohmol@gmail.com>",
    "url": "https://github.com/paed01"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "lib/",
    "index.js"
  ],
  "scripts": {
    "test": "mocha -R dot",
    "posttest": "eslint . --cache && npm run toc",
    "wintest": "node_modules/.bin/mocha",
    "toc": "node scripts/generate-api-toc ./docs/API.md,./docs/Examples.md",
    "test-md": "node scripts/test-markdown.js ./docs/API.md && node scripts/test-markdown.js ./docs/Examples.md",
    "build": "rollup -c",
    "dist": "npx babel index.js -d dist && rm -rf dist/lib && npx babel lib -d dist/lib",
    "server": "nodemon -r @babel/register -r @babel/polyfill  docs/examples/server.js",
    "examples": "node -r @babel/register -r @babel/polyfill  docs/examples/run.js"
  },
  "keywords": [
    "workflow",
    "engine",
    "process",
    "automation",
    "bpmn",
    "bpmn 2"
  ],
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/paed01/bpmn-engine/master/LICENSE"
    }
  ],
  "nyc": {
    "exclude": [
      "test"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "babel-loader": "^8.0.0-beta.6",
    "bent": "^7.0.2",
    "chai": "^4.1.2",
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "express": "^4.17.1",
    "markdown-toc": "^1.2.0",
    "mocha": "^6.2.1",
    "mocha-cakes-2": "^3.3.0",
    "nock": "^11.4.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "unnecessary": "^1.3.4"
  },
  "dependencies": {
    "bpmn-elements": "^0.11.0",
    "bpmn-moddle": "^6.0.0",
    "camunda-bpmn-moddle": "^4.3.0",
    "debug": "^4.1.1",
    "moddle-context-serializer": "^0.11.0",
    "smqp": "^1.10.0"
  }
}
